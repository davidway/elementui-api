package com.blockchain.VO;

import javax.enterprise.inject.New;
import javax.net.ssl.SSLEngineResult.Status;

import com.blockchain.exception.StatusCode;

public class PhpSystemJsonContent {
	Integer retcode;
	String retmsg;
	Object data = new Object();

	
	public PhpSystemJsonContent(){
		this.retcode=0;
		this.retmsg="成功";
		this.data=data;
	}
	
	
	public Integer getRetcode() {
		return retcode;
	}


	public void setRetcode(Integer retcode) {
		this.retcode = retcode;
	}


	public String getRetmsg() {
		return retmsg;
	}
	public void setRetmsg(String retmsg) {
		this.retmsg = retmsg;
	}
	public Object getData() {
		return data;
	}
	public void setData(Object data) {
		this.data = data;
	}


	public PhpSystemJsonContent setParameterError(String s ) {
		this.retcode=StatusCode.PARAM_ERROR;
		this.retmsg=s;
	
		return this;
	}


	public PhpSystemJsonContent setSDKError() {
		this.retcode=StatusCode.SYSTEM_UNKOWN_ERROR;
		this.retmsg="SDK环境异常，请联系管理员";
	
		return this;
		
	}


	public PhpSystemJsonContent setParseJsonError() {
		this.retcode=StatusCode.SERVICE_EXCEPTION;;
		this.retmsg="json解析错误";
		
		return this;
	}


	public PhpSystemJsonContent setUnkownError(String message) {
		this.retcode=StatusCode.SYSTEM_UNKOWN_ERROR;
		this.retmsg="未知错误";
		
		return this;
	}


	@Override
	public String toString() {
		return "PhpSystemJsonContent [retcode=" + retcode + ", retmsg=" + retmsg + ", data=" + data + "]";
	}
	
	
}
